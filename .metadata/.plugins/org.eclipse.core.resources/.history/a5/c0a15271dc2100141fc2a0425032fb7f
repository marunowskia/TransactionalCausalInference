<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd">

	
	<entity class="oss.marunowskia.datamining.transactionalcausalinference.models.TraceData">
		<named-native-query name="GetTraceData"
			result-class="oss.marunowskia.datamining.transactionalcausalinference.models.TraceData">
			<query>
				<![CDATA[ 
					SELECT top 10000
						[RowNumber]
				      ,	[TextData]
				      ,	[Duration]
				      ,	[StartTime]
				      ,	[EndTime]
				  from 
				  	research..full_trace WITH (NOLOCK)
				  where
				  		starttime is not null
				  	and
				  		endtime is not null
				  	and 
				  		textdata is not null
				  and
				  	starttime > :starting_point
				  order by StartTime, EndTime
				]]>
			</query>
		</named-native-query>
	</entity>
</entity-mappings>
