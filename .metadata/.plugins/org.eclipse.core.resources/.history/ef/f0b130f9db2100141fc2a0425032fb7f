	<entity class="edu.cwru.eecs435.amarunowski.analysis.models.TraceData">
		<named-native-query name="GetTraceData"
			result-class="edu.cwru.eecs435.amarunowski.analysis.models.TraceData">
			<query>
				<![CDATA[ 
					SELECT top 10000
						[RowNumber]
				      ,	[TextData]
				      ,	[Duration]
				      ,	[StartTime]
				      ,	[EndTime]
				  from 
				  	research..full_trace WITH (NOLOCK)
				  where
				  		starttime is not null
				  	and
				  		endtime is not null
				  	and 
				  		textdata is not null
				  and
				  	starttime > :starting_point
				  order by StartTime, EndTime
				]]>
			</query>
		</named-native-query>
	</entity>
